<div class="span5 well">
  <h4>Distributors</h4>
  <br/>
  <% @distributors.each do |distributor| %>
    <h4><li><%= link_to distributor.name, distributor %></li></h4>
  <% end %>
  <hr>
  <% if logged_in? %>
    <%= link_to "Add Distributor", new_distributor_path(@distributor) %>
  <% end %>
</div>
<div class="span5 well">
  <h4>Products</h4>
  <% @goods.each do |good| %>
    <li><%= link_to good.name, good %></li>
  <% end %>
</div>