<div class="span5 well">
  <%= simple_form_for '/login', class: "form-signin" do %>
  <div class="control-group">
    <%= label_tag :username %>
    <%= text_field_tag :username %>
  </div>
  <div class="control-group">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>
    <%= submit_tag 'Login', class: 'btn btn-success' %>
  <% end %>
</div>


